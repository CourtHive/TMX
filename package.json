{
  "name": "tmx",
  "license": "MIT",
  "private": true,
  "version": "3.0.54",
  "type": "module",
  "engines": {
    "node": "22",
    "pnpm": ">=8",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "analyze": "source-map-explorer 'dist/**/*.js'",
    "build": "rimraf dist && tsc && vite build",
    "build:dev": "rimraf dist && tsc && vite build --mode development",
    "build:prod": "rimraf dist && tsc && vite build --mode production && npm version $(semver $npm_package_version -i patch) && pnpm prepublishOnly",
    "check-types": "tsc --noEmit",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "config": "node config.mjs",
    "prepublishOnly": "./addVersion $npm_package_version",
    "docs:publish": "pnpm build:prod && git checkout docs && cp -r dist/* docs && git add docs && git commit -m 'docs: update' && git push && git checkout main",
    "format": "prettier --write src",
    "lint": "eslint src --fix --cache",
    "lint:staged": "TZ=UTC lint-staged",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "preview": "vite preview",
    "postbuild": "./addVersion $npm_package_version",
    "start": "pnpm run config --check && vite --open",
    "lint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "test": "TZ=UTC vitest",
    "e2e": "cypress run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/plugin-syntax-flow": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@types/babel__traverse": "7.28.0",
    "@types/node": "22.18.10",
    "@types/socket.io-client": "3.0.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "assert": "2.1.0",
    "buffer": "6.0.3",
    "c8": "10.1.3",
    "crypto-browserify": "3.12.1",
    "cypress": "13.17.0",
    "cz-git": "1.12.0",
    "envfile": "7.1.0",
    "eslint": "8.57.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-sonarjs": "3.0.5",
    "esm": "3.2.25",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "inquirer": "12.10.0",
    "lint-staged": "16.2.4",
    "os-browserify": "0.3.0",
    "prettier": "3.6.2",
    "process": "0.11.10",
    "rimraf": "6.0.1",
    "semver": "7.7.3",
    "source-map-explorer": "2.5.3",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "typescript": "5.9.3",
    "url": "0.11.4",
    "vite": "5.4.20",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.1.9",
    "vitest-github-actions-reporter": "0.11.1",
    "vitest-sonar-reporter": "2.0.4",
    "yargs": "17.7.2"
  },
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "7.27.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@event-calendar/core": "3.12.0",
    "@event-calendar/day-grid": "3.12.0",
    "@event-calendar/interaction": "3.12.0",
    "@event-calendar/list": "3.12.0",
    "@event-calendar/resource-time-grid": "2.7.2",
    "@event-calendar/time-grid": "3.12.0",
    "@stitches/core": "1.2.8",
    "animate.css": "4.1.1",
    "awesomplete": "1.1.7",
    "axios": "1.12.2",
    "bulma": "1.0.4",
    "bulma-checkradio": "2.1.3",
    "bulma-switch": "2.0.4",
    "camelcase": "8.0.0",

    "qs": "6.11.1",
    "quill": "1.3.7",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-scripts": "5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "2.4.1",
    "tabulator-tables": "^5.5.0",

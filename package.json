{
  "name": "tmx",
  "license": "MIT",
  "private": true,
  "version": "3.1.30",
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=8",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "build": "rimraf dist && tsc && vite build",
    "build:dev": "rimraf dist && tsc && vite build --mode development",
    "build:prod": "rimraf dist && tsc && vite build --mode production && npm version $(semver $npm_package_version -i patch) && pnpm prepublishOnly",
    "build:minor": "rimraf dist && tsc && vite build --mode production && npm version $(semver $npm_package_version -i minor) && pnpm prepublishOnly",
    "check-types": "tsc --noEmit",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "config": "node config.mjs",
    "prepublishOnly": "./addVersion $npm_package_version",
    "docs:publish": "pnpm build:prod && git checkout docs && rm -rf docs/* && cp -r dist/* docs && git add docs && git commit -m 'docs: update' && git push && git checkout main",
    "docs:minor": "pnpm build:minor && git checkout docs && rm -rf docs/* && cp -r dist/* docs && git add docs && git commit -m 'docs: update' && git push && git checkout main",
    "format": "prettier --write src",
    "lint": "eslint src --fix --cache",
    "lint:staged": "TZ=UTC lint-staged",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "preview": "vite preview",
    "postbuild": "./addVersion $npm_package_version",
    "start": "pnpm run config --check && vite --open",
    "lint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "test": "TZ=UTC vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/plugin-syntax-flow": "7.28.6",
    "@babel/preset-typescript": "7.28.5",
    "@commitlint/cli": "20.4.2",
    "@commitlint/config-conventional": "20.4.2",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/html": "^10.1.11",
    "@storybook/html-vite": "^10.1.11",
    "@types/babel__traverse": "7.28.0",
    "@types/node": "24.10.13",
    "@types/pdfmake": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "assert": "2.1.0",
    "buffer": "6.0.3",
    "c8": "10.1.3",
    "crypto-browserify": "3.12.1",
    "cz-git": "1.12.0",
    "envfile": "7.1.0",
    "eslint": "10.0.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-sonarjs": "4.0.0",
    "esm": "3.2.25",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "inquirer": "13.2.5",
    "lint-staged": "16.2.7",
    "os-browserify": "0.3.0",
    "prettier": "3.8.1",
    "process": "0.11.10",
    "rimraf": "6.1.3",
    "semver": "7.7.4",
    "storybook": "^10.1.11",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "typescript": "5.9.3",
    "url": "0.11.4",
    "vite": "7.3.1",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "4.0.18",
    "vitest-github-actions-reporter": "0.11.1",
    "vitest-sonar-reporter": "3.0.0",
    "yargs": "18.0.0"
  },
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "7.28.6",
    "@eslint/js": "^10.0.1",
    "@event-calendar/core": "3.12.0",
    "@event-calendar/day-grid": "3.12.0",
    "@event-calendar/interaction": "3.12.0",

    "vanillajs-datepicker": "^1.3.3"
  },
  "scripts": {
    "lint": "eslint src --fix --cache",
    "lint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "format": "prettier --write src",
    "start": "PORT=3333 react-app-rewired start",
    "build": "NODE_ENV=production react-app-rewired build",
    "pwa": "cp ./src/indices/index.pwa.js ./src/index.js",
    "plus": "cp ./src/indices/index.plus.js ./src/index.js",
    "build:major": "npm version $(semver $npm_package_version -i major) && NODE_ENV=production react-app-rewired build",
    "build:minor": "npm version $(semver $npm_package_version -i minor) && NODE_ENV=production react-app-rewired build",
    "build:patch": "npm version $(semver $npm_package_version -i patch) && NODE_ENV=production react-app-rewired build",
    "postbuild": "./addVersion $npm_package_version",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/babel__traverse": "7.18.2",
    "@types/node": "11.15.35",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/socket.io-client": "1.4.35",
    "@types/uuid": "3.4.9",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.39.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "esm": "3.2.25",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "os-browserify": "^0.3.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.1"
  }
}

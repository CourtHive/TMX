    "tabulator-tables": "6.3.1",
    "timepicker-ui": "2.6.1",
    "tippy.js": "6.3.7",
    "tods-competition-factory": "2.2.30",
    "typescript": "5.9.3",
    "vanillajs-datepicker": "1.3.4"
  }
}
